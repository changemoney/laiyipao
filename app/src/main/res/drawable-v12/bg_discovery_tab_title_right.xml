<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#ff6f5d54" />
            <corners android:topRightRadius="12.5dip" android:bottomRightRadius="12.5dip" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:topRightRadius="12.5dip" android:bottomRightRadius="12.5dip" />
            <stroke android:width="1.0dip" android:color="#ff6f5d54" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>